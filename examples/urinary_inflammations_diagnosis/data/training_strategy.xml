<TrainingStrategy>
   <Main Type="CONJUGATE_GRADIENT">
      <TrainingDirectionMethod>FR</TrainingDirectionMethod>
      <LearningRateAlgorithm>
         <LearningRateMethod>BrentMethod</LearningRateMethod>
         <LearningRateTolerance>0.001</LearningRateTolerance>
      </LearningRateAlgorithm>
      <ReturnMinimumSelectionErrorNN>0</ReturnMinimumSelectionErrorNN>
      <ApplyEarlyStopping>1</ApplyEarlyStopping>
      <MinimumParametersIncrementNorm>0</MinimumParametersIncrementNorm>
      <MinimumLossDecrease>1e-06</MinimumLossDecrease>
      <LossGoal>0.001</LossGoal>
      <GradientNormGoal>0</GradientNormGoal>
      <MaximumSelectionErrorIncreases>1000000</MaximumSelectionErrorIncreases>
      <MaximumEpochsNumber>1000</MaximumEpochsNumber>
      <MaximumTime>1000</MaximumTime>
      <ReserveTrainingErrorHistory>1</ReserveTrainingErrorHistory>
      <ReserveSelectionErrorHistory>0</ReserveSelectionErrorHistory>
   </Main>
</TrainingStrategy>
